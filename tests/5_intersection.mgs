# Points
createPoint((1,2,4), "A")
createPoint((3,0.5,0), "B")
createPoint((-2,1,None), "C")
createPoint((0,1,None), "D")
createPoint((-2,None,1), "G")
createPoint((0,None,4), "H")

# Lines
createLine("A1", "B1", "m1")
createLine("A2", "B2", "m2")
createLine("C1", "D1", "k1")
createLine("G2", "H2", "_k2")

# Intersections lines to lines
intersect("m1", "k1", "I1")
intersect("m2", "_k2", "P2")

# Intersections lines to circles
createCircle("A1", 2, "r1")

# by default returns first intersection
intersect("m1", "r1", "Q1")
# returns second intersection
intersect("m1", "r1", "X1", 2)
intersect("k1", "r1", "T1", 2)

# also Intersections can be circle to circle
createCircle("X1", 1, "s1")
intersect("r1", "s1", "Y1", 1)
# and result two
intersect("r1", "s1", "Z1", 2)
