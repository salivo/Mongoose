# Points
createPoint((1,2,5), "A")
createPoint((3,1,0), "B")
createPoint((-2,1,None), "C")
createPoint((0,1,None), "D")
# Lines
createLine("A1", "B1", "m1")
createLine("A2", "B2", "m2")
createLine("C1", "D1", "k")
createPoint((-2,None,1), "G")
createPoint((0,None,4), "H")
createLine("G2", "H2", "l")
createLine("A2", "D1", "m")
# To set style use
# setType("line to not show", "none") to not draw line use "none"
setType("m", "hidden")
setType("m1", "realsized")
setStyle("m2", "bold")

# Also to simplyfy creation of planes (three points and two lines) we can simply do this
createPlane((7,4.5,7), "rho")
# then we can use it lines with rho1 and rho2
